            <div class="navbar-header" style="color:white" id="nav_head">
                    <img src="/Images/landingPage_images/projectlogo.svg" alt="logo" width="40px" height="40px" style="padding-right:0px;padding-bottom: 4px">
                    <div class="navbar-brand">WorkSpace</div>
                    
            </div>          
</nav>

<div class="d-none d-sm-block">
        <div class="headerHolder text-center mt-5 pt-5 ml-4" style="width: 97vw">
                <center>
                        <h4 class="pageHead" style="width: 300px">WS TEAM <span style="color: blue">SELECTION</span></h4>
                </center>
                          
            </div>
            
            <div class="container-fluid mainDivLg mt-4 pt-2">
                    
                <div class="row">
                    
                    <div class="col-6 contentHolderLgOne">
                        <img class="mb-4" src="/Images/wsTeam/vacancyLg.svg" alt="Failed to load" height="270px" width="270px">
                        <a href="/workspace/freelancer/joinTeam/<%= frlData.detailsOwner %>" class="link1" style="color: black;text-decoration: none;font-size: 1.2em"><b>JOIN A TEAM</b></a>
                    </div>
                    <div class="col-6 contentHolderLgTwo">
                            <img src="/Images/wsTeam/teamworkLg.svg" style="margin-bottom: 28px" alt="Failed to load" height="245px" width="245px">
