                <center>
                    <a href="/workspace/freelancer/newTeam" class="wsLinks" style="color: white;text-decoration: none"><b>CREATE A TEAM</b></a>
                </center>
            </div>
        </div>
        <div class="col-12  contentHolder">
            <div class="joinTeam">
                <center>
                    <img class="imgMargin d-none d-sm-block" style="margin-bottom: 62px" src="/Images/wsTeam/joinTeam.png" alt="Failed to load image" width="400px" height="300px">
                </center>

                <center>
                    <img class="mb-5 d-block d-sm-none" style="margin-top: -20px" src="/Images/wsTeam/vacancy.svg" alt="Failed to load image"  width="250px" height="250px">
                </center> 

                <center>
                    <a href="/workspace/freelancer/joinTeam/<%= frlData.detailsOwner %>" class="wsLinks" style="color: white;text-decoration: none"><b>JOIN A TEAM</b></a>
                </center>
            </div>
        </div>
    </div>

