    <div class="container-fluid">
        <div class="row">
                <div class="col-12 col-md-5 blueBox">
                    <center>
                        <img  src="<%= cad.Cimage %>" alt="Failed to load image" width="200px" height="200px" style="border-radius: 50%;margin-top: 29vh;">
                    </center>
                    <center>
                        <h4 style="color: white;" class="mt-3">Hello we are <%= cad.Cname %> a <br><b><%= cad.companyType %></b> company.</h4>
                    </center>
                    <center>
                        <h6 class="mt-3" style="color: white;">EST. <%= cad.established %></h6>
                    </center>

                    <center>
                            <i class="fas fa-angle-double-down d-block d-sm-none fa-lg " style="margin-top: 80px;color:white"></i>
                    </center>
                </div>
                <div class="col-12 col-md-7 whiteBox p-3 p-md-5">
