<div class="mainDiv">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12 col-md-4 blueBox">
              <center>
                <i class="fas fa-project-diagram fa-2x" style="padding-top: 240px;color:white"></i>
                <h1 style="color: white;font-weight: bold;display: inline;padding-left: 5px;"><%= pd.pName %></h1>
              </center>
              <center>
                <p class="pl-4 pr-4 mt-3" style="font-size: 1.4em;color:whitesmoke">Please enter a detailed review for the team members. Your reviews helps us to develope an optimum work enviroment for you.</p>
              </center>
              <% if(success && success.length > 0 ){%>
      
        
                <div class="alert alert-info p-1 pos mt-5" style="width:50%;text-align: center;margin-top: -40px;margin-left:100px" role="alert">
                  <b ><%= success %></b>
                </div>
              
              
            <% } %>
            </div>
            <div class="col-12 col-md-8 whiteBox pt-5 pl-0 pl-md-5">
                <div class="row mt-4">
                  <div class="scrollDiv pr-3 pl-1 pt-2">
                    <div class="frlBox">
                      <div class="row">
                        <div class="col-4">
                          <center>
                            <i class="fas fa-users fa-3x mt-5 pt-4"></i>
                          </center>
                          <center>
                            <h4 class="mt-2"><b><%= wsd.teamName %> </b></h4>
                          </center>
                        </div>
                        <div class="col-8">
                            <form action="/workspace/project/teamReview/<%= wsd._id %>/<%= pid %>/<%= pname %>/<%= uname %>/<%= utype %>" method="POST">
                              <textarea name="rTxt" id="" cols="60" rows="6" class="mt-2 p-2 txtArea" placeholder="Enter your review here." required></textarea>
                              <input type="number" name="stars" id="" class="mt-2 ratingBox pl-2" placeholder="Enter your rating" min="1" max="5" required>
                              <button type="submit" class="subBtn">SUBMIT REVIEW</button>
                            </form>
                        </div>
                      </div>   
                    </div>
                      <% fd.forEach(function(frlDtl){ %>
                        <% for(var i = 0 ; i < pd.memberList.length;i++){ %>
                          <% if(frlDtl.detailsOwner == pd.memberList[i].usrID ){ %>
                            <div class="frlBox">
                              <div class="row">
                                <div class="col-4">
                                  <center>
                                    <img src="<%= frlDtl.Fimage %>" style="border-radius: 50%;" alt="" width="150px" height="150px" class=" mt-4">
                                  </center>
                                  <center>
                                    <h4 class="mt-2"><b><%= frlDtl.fname %> <%= frlDtl.lname %></b></h4>
                                  </center>
                                </div>
                                <div class="col-8">
                                    <form action="/workspace/project/wsTeamReview/<%= wsd._id %>/<%= frlDtl.detailsOwner %>/<%= pid %>/<%= pname %>/<%= uname %>/<%= utype %>" method="POST">
                                      <textarea name="rTxt" id="" cols="60" rows="6" class="mt-2 p-2 txtArea" placeholder="Enter your review here." required></textarea>
                                      <input type="number" name="stars" id="" class="mt-2 ratingBox pl-2" placeholder="Enter your rating" min="1" max="5" required>
                                      <button type="submit" class="subBtn">SUBMIT REVIEW</button>
                                    </form>
                                </div>
                              </div>   
                            </div>
                          <% } %>
                         
                        <% } %>
                      <% }); %>
                  </div>
                </div>     
            </div>
        </div>
    </div>
</div>


