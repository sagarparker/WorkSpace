<!DOCTYPE html>
<html lang="en">
<head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <link rel="shortcut icon" type="image/x-icon" href="/Images/landingPage_images/projectlogo.png"/>
        <title>WorkSpace</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
    
     
        
    <!-- ANIMATE CSS LINK -->
    
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.0/animate.min.css">
    
    <!-- DEFAULT CSS -->

    <link rel="stylesheet" href="/CSS/pageDefault.css">
    
    
    <!-- BOOTSTRAP CSS -->
    
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    
    <!-- BOOTSTRAP JQUERY,JS -->
    
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.0/css/all.css" integrity="sha384-Mmxa0mLqhmOeaE8vgOSbKacftZcsNYDjQzuCOm6D02luYSzBG8vpaOykv9lFQ51Y" crossorigin="anonymous">
    <link rel="stylesheet" href="/CSS/loadingAni.css">
    <link rel="stylesheet" href="/CSS/freelancer/reqTeamDetails.css">
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="/JS/loadingAni.js"></script>
    <script src="/JS/defaultPage.js"></script>
    
</head>
<div class="loader">
        <i class="fas fa-sync-alt fa-3x fa-spin"></i>
</div>

<body>


<nav class="navbar  navbar-dark  navbar-expand-sm fixed-top" id="navani">

            <div class="navbar-header" style="color:white" id="nav_head">
                    <img src="/Images/landingPage_images/projectlogo.svg" alt="logo" width="40px" height="40px" style="padding-right:0px;padding-bottom: 4px">
                    <div class="navbar-brand">WorkSpace</div>
                    
            </div>

            
    
            <button class="navbar-toggler" data-toggle="collapse" data-target="#cnb" id="hamBtn">
              <img src="/CSS/menu.svg" alt="" width="20px" >
                    
            </button>
    
            
                
            
            
            <div class="collapse navbar-collapse" id="cnb">
            
                <ul class="navbar-nav" >
                
              

                <li class="nav-item">
                    <a class="nav-link" href="/workspace/contact" target="_blank" onMouseOver="this.style.color='blue'" onMouseOut="this.style.color='white'" style="color:white">Contact</a>
                </li>

             

                
                
                
                </ul>
            </div> 

        
                
                
</nav>

<div class="container-fluid mainDiv">
        <div class="row">
            <div class="col-12 col-md-5 teamInfo">
                <h1 class="mt-5" ><b><%= reqWsTeam.teamName %></b> </h1>
                <h3 class="mt-2"><%= reqWsTeam.teamCategory %></h3>
                <% if(reqWsTeam.wsTeamReviews){ %>
                    <script>
                        var rating = 0;
                    </script>
                
                <% reqWsTeam.wsTeamReviews.forEach(function(wsd){ %>
                    <script>
                        rating = rating + <%= wsd.stars %> 
                    </script>
                 
                <% });  %>
                    <h4 class="mt-5"><span style="font-weight: bold;"> Team rating : </span>
                        <script>
                            document.write(rating/<%= reqWsTeam.wsTeamReviews.length %>);
                        </script>
                    </h4>
                  
                <% } %>

                <h4 class="mt-5"><b>No of freelancers in team</b> : <%= reqWsTeam.teamMembers.length %></h4>
                <center>
                        <h4 class="mt-4 mt-md-1 "><b>No of projects done by the team</b> : 12</h4>                                          
                </center>
               
                <form action="/workspace/freelancer/sendReq/<%= reqWsTeam.teamAdmin %>/<%= reqWsTeam.teamName %>/<%= reqWsTeam._id %>" method="POST">
                    <button type="submit" class="joinTeam mt-5">Send request</button>
                </form>
    
            </div>
            <div class="col-12 col-md-7">
                <div class="teamMembersHolder mt-5 pt-4">
                    <div class="scrollDiv">
                            <% reqFreelancerDetl.forEach(function(frlData){ %>
                                <% for(var i = 0 ; i < reqWsTeam.teamMembers.length ; i++){ %>
                                    <% if( reqWsTeam.teamMembers[i].memberID == frlData.detailsOwner ) { %>
                                        <div class=" container-fluid teamMembersContainer mb-3">
                                            <div class="row">
                                                <div class="col-12 col-md-3 toCenter">
                                                    <img src="<%= frlData.Fimage%>" style="border-radius: 50%" class="ml-0 ml-md-3" height="80px" width="80px" alt="Failed to load">
                                                </div>
                                               
                                                <div class="col-12 col-md-6 pt-1 toCenter">
                                                    
                                                            <h3 style="font-weight: bold"><%= frlData.fname %> <%= frlData.lname %></h3>
                                                
                                                   
                                                     
                                                            <h4 class="mt-1 centerH4" ><%= frlData.designation %></h4>
                                                     
                                                      
                                                </div>
                                                <div class="col-12 col-md-3 toCenter">
                                                    <h4 class="pt-1"><b>Rating:</b></h4>
                                                    <% if(frlData.frlReviews){ %>
                                                        <script>
                                                            var ratingFrl = 0;
                                                        </script>
                                                    
                                                    <% frlData.frlReviews.forEach(function(frl){ %>
                                                        <script>
                                                            ratingFrl = ratingFrl + <%= frl.stars %> 
                                                        </script>
                                                     
                                                    <% });  %>
                                                        <h4 class="pt-2" style="color: blue;">
                                                            <script>
                                                                document.write(ratingFrl/<%= frlData.frlReviews.length %>);
                                                                var ratingFrl = 0;
                                                            </script>
                                                        </h4>
                                                      
                                                    <% } %>
                                                </div>
                                            </div>
                                                
                                        </div>
                                    
                                    <% } %>
                                <% } %>
                            <% }); %>
                    </div>
                </div>
         
            </div>
        </div>
    
    </div>


<!-- FOOTER -->

<% include partials/footer %>


                          
    
</body>
</html>