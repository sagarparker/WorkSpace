<div class="container cont mt-3 pt-3 pt-sm-2">
    <div class="row d-flex align-items-center">
        <div class="col-12 col-sm-7">
        <div class="formCont  mt-0 mt-sm-3">
            <div class="fc">
                    <div class="col-12 text-center" style="font-size: 1.7em;font-weight: bolder">FREELANCER DETAILS</div>
                    <form action="/workspace/freelancerdetails" method="POST" class="pl-3 pr-3 pl-sm-5 pr-sm-5" enctype="multipart/form-data">
                        <br>
                        <input type="text" id="fname" name="fname" class="form-control" placeholder="Firstname" autocomplete="off" required><br>
                        <input type="text" id="lname" name="lname" class="form-control" placeholder="Lastname" autocomplete="off" required><br>
                        <input type="number" id="age" name="age" class="form-control" placeholder="Age" autocomplete="off" required><br>
                        
                        <label for="desg" ><b>Designation:</b></label>
                        <select name="designation" id="desg" class="form-control">
                            <option value="" disabled selected>Select your designation</option>
                            <option value="Project manager">Project manager</option>
                            <option value="Full-stack Web-developer">Full-stack Web-developer</option>
                            <option value="Frontend Web-Developer">Frontend Web-Developer</option>
                            <option value="Backend Web-Developer">Backend Web-Developer</option>
                            <option value="Mobile application developer">Mobile application scientist</option>
                            <option value="Developer (PYTHON)">Developer (PYTHON)</option>
                            <option value="Developer (JAVASCRIPT)">Developer (JAVASCRIPT)</option>
                            <option value="Developer (JAVA)">Developer (JAVA)</option>
                            <option value="Developer (C/C++/C#)">Developer (C++/C#)</option>
                            <option value="UI/UX designer">UI/UX designer</option>
                            <option value="Graphic designer">Graphic designer</option>
                            <option value="Adobe artist">Adobe artist</option>
                            <option value="Video editor">Video editor</option>
                            <option value="Data scientist">Data scientist</option>
                            
                        </select>
        
                        <br>
                        <label for="edu"><b>Education:</b></label>
                        <select name="education" id="edu" class="form-control">
                            <option value="" disabled selected>Select your education</option>
                            <option value="SSC">SSC</option>
                            <option value="HSC">HSC</option>
                            <option value="Under-Graduate">Under-Graduate</option>
                            <option value="Graduate">Graduate</option>
                        </select>

                        <br>
                        <div class="form-group">
                            <label for="image"><b>Profile picture: </b></label>
                            <input type="file" id="image" name="image" accept="image/*"  required>
                        </div>
                       
                        <div class="row justify-content-center">
                            
                                <div class="col-8 col-sm-4">
                                        <input type="submit" id="subBtn" value="SUBMIT" class="btn btn-primary form-control"> 
                                </div>
                                    
                        
                        </div>
            </div>
            
                  
        </div>
               

        </form>
        </div>
        <div class="col-5 d-none d-sm-block pt-2">
            <img src="/Images/userDetails_images/signup.svg" width="500px" height="500px" alt="">
        </div>
        </div>
</div>



