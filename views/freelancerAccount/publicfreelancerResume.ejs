<div class="mainDiv">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12 col-md-5 blueBox">
               
                <center>
                        <img src="<%= rData.Fimage %>" alt="Failed to load image" height="250px" width="250px" style="border-radius: 50%;margin-top: 23vh;">
                </center>
                <center>
                        <h4 class="pl-5 pr-5 mt-4" style="color: white;">Hello I'm <%= rData.fname %> <%= rData.lname %> a <b><%= rData.designation%>.</b></h4>
                </center>
                <center>
                        <i class="fas fa-angle-double-down d-block d-sm-none fa-lg " style="margin-top: 70px;"></i>
                </center>
            </div>
            <div class="col-12 col-md-7 whiteBox pt-5 pl-0 pl-md-5">
                <div class="row mt-4">
                    <div class="col-6 pl-4 pl-md-4">
                            <h4 class="mt-5  Rtitle" >Designation</h4>
                            <h4><%= rData.designation %></h4>
                    </div>
                    <div class="col-6 pl-5">
                            <h4 class="mt-5 Rtitle ">Education</h4>
                            <h4><%= rData.education %></h4>

                    </div>

                  

                </div>
                <div class="row mt-5">
                    <div class="col-6 pl-4  mt-3">
                        <h4 class="Rtitle">Skills</h4>
                        <ul style="margin-left: -18px;">
                            <% rData.skills.forEach(function( rSkills){ %>
                           
                                <li style="font-size:1.2em ;"><%= rSkills.newSkill %></li>
                        
                            <% }); %>
                        </ul>
                    </div>
                    <div class="col-6 pl-5 mt-3">
                        <h4 class="Rtitle" >Hobbies</h4>
                        <ul style="margin-left: -18px;">
                            <% rData.hobbies.forEach(function( rHob){ %>
                            
                                <li style="font-size:1.2em ;"><%= rHob.newHobbies %></li>
                        
                            <% }); %>
                        </ul>
                    </div>
                </div>

                <div class="row mt-5">
                    <div class="col-6 pl-4">
                        <h4 class=" Rtitle">Experiences</h4>
                        <ul style="margin-left: -18px;">
                                <% rData.experience.forEach(function( rExp){ %>
                                
                                    <li style="font-size:1.2em ;"><%= rExp.newExp %></li>
                            
                                <% }); %>
                        </ul>
                    </div>
                    <div class="col-6 pl-5">
                        <h4 class="Rtitle ">Joined WorkSpace on</h4>
                        <h5><%= rData.FacCdate %></h5>
                    </div>
                </div>
                  

                   
            </div>
        </div>
    </div>
</div>


